<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping> 
	<class name="fr.esiag.isies.pds.model.congestion.EmergencyActivity" table="tb_emergency_activity_simulation">
		<id name="id" type="int">
			<column name="id_activity_emergency" precision="11" scale="0" />
			<generator class="increment" />
		</id>
		<property name="idPatient" type="int">
			<column name="id_patient" length="255" not-null="false" />
		</property>
		<property name="idDoctor" type="int">
			<column name="id_doctor" length="255" not-null="true" />
		</property>
		<property name="idRoom" type="int">
			<column name="id_salle" length="255" not-null="true" />
		</property>
		<property name="dateEntree" type="timestamp">
			<column name="date_entree" length="255" not-null="true" />
		</property>
		<property name="dateSortie" type="timestamp">
			<column name="date_sortie" length="255" not-null="false" />
		</property>
	</class>
</hibernate-mapping>