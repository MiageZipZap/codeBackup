<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class
		name="fr.esiag.isies.pds.model.Person"
		table="tp_common_person" dynamic-insert="true">
		<id name="id" type="int">
			<column name="idtp_common_person" precision="5"
				scale="0" />
			<generator class="increment" />
		</id>
		<property name="name" type="string">
			<column name="tp_common_person_name" length="45" not-null="false" />
		</property>
		<property name="surname" type="string">
			<column name="tp_common_person_surname" length="45" not-null="false" />
		</property>
		<property name="age" type="string">
			<column name="tp_common_person_age" length="45" not-null="false" />
		</property>
		<property name="sex" type="string">
			<column name="tp_common_person_sex" length="45" not-null="false" />
		</property>
		<property name="tel" type="string">
			<column name="tp_common_person_tel" length="45" not-null="false" />
		</property>
	</class>
</hibernate-mapping>