<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="fr.esiag.isies.pds.model.hospital.logistics.MapActMedicalResource"
		table="tr_map_act_medical_resource">
		<id name="id" type="int">
			<column name="idtr_map_act_medical" precision="5" scale="0" />
			<generator class="increment" />
		</id>
		<many-to-one name="typeRefInfraEquipment"
			class="fr.esiag.isies.pds.model.referential.infrastructure.TypeRefInfra"
			lazy="false" column="idtr_typeRefInfraEquipment_ref_map" not-null="false" />
	<many-to-one name="typeRefInfraInfra"
			class="fr.esiag.isies.pds.model.referential.infrastructure.TypeRefInfra"
			lazy="false" column="idtr_typeRefInfraInfra_ref_map" not-null="false" />

	</class>
</hibernate-mapping>