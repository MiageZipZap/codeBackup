<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="fr.esiag.isies.pds.model.waitingqueue.WaitingQueue" table="tp_waiting_queue" dynamic-update="true">
		
		<id name="id" type="int">
			<column name="idtp_waiting_queue" precision="5" scale="0" />
			<generator class="increment" />
		</id>
				
		<property name="idOrganization" type="int">
			<column name="tp_waiting_queue_id_organization" length="5" not-null="true" />
		</property>
		
		<property name="idService" type="int">
			<column name="tp_waiting_queue_id_service" length="5" not-null="true" />
		</property>
		
		<property name="timeQueueState" type="java.util.Date">
			<column name="tp_waiting_queue_time_queue_state" length="255" not-null="true" sql-type="datetime" default="now()" />
		</property>
		
		<property name="idPatient" type="int">
			<column name="tp_waiting_queue_id_patient" length="10" not-null="true" />
		</property>
		
		<property name="priority" type="int">
			<column name="tp_waiting_queue_priority" length="1" not-null="true" />
		</property>
		
		<property name="idMedicalAct" type="int">
			<column name="tp_waiting_queue_medical_act" length="5" not-null="true" />
		</property>
		
		<property name="idBox" type="int">
			<column name="tp_waiting_queue_id_box" length="5" not-null="true" />
		</property>
		
		<property name="idDoctor" type="int">
			<column name="tp_waiting_queue_id_doctor" length="5" not-null="true" />
		</property>
		
		<one-to-one name="patient"
			class="fr.esiag.isies.pds.model.referential.person.patient.Patient"
			cascade="save-update" />
			

	</class>
</hibernate-mapping>