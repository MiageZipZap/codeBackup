<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class
		name="fr.esiag.isies.pds.model.referential.medicalRecord.MedicalRecord"
		table="tr_medical_record" dynamic-insert="true">
		<id name="id" type="int">
			<column name="IDTR_MEDICAL_RECORD" precision="5" scale="0" />
			<generator class="increment" />
		</id>
		<property name="height" type="string">
			<column name="tr_medical_record_number" length="45" not-null="false" />
		</property>
		<property name="weight" type="string">
			<column name="tr_medical_record_weight" length="45" not-null="false" />
		</property>
		<property name="bloodGroup" type="string">
			<column name="tr_medical_blood_group" length="45" not-null="false" />
		</property>
		<one-to-one name="patient"
			class="fr.esiag.isies.pds.model.referential.person.patient.Patient"
			cascade="save-update" />
	</class>
</hibernate-mapping>