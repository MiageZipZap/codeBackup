<hibernate-mapping>
	<class name="fr.esiag.isies.pds.model.referential.staff.StaffMember" table="TR_STAFF_MEMBER" dynamic-update="true">
		<id name="id" type="int">
			<column name="IDTR_STAFF_MEMBER" precision="5" scale="0" />
			<generator class="increment" />
		</id>
		
		<one-to-one name="adress" class="fr.esiag.isies.pds.model.Adress"/>
		<property name="staffNumber" type="string">
			<column name="TR_STAFF_MEMBER_NUMBER" length="45" not-null="false" />
		</property>
		<property name="firstName" type="string">
			<column name="TR_STAFF_MEMBER_FIRST_NAME" length="45" not-null="false" />
		</property>
		<property name="lastName" type="string">
			<column name="TR_STAFF_MEMBER_LAST_NAME" length="45" not-null="false" />
		</property>
		

