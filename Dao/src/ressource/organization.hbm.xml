<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="fr.esiag.isies.pds.model.referential.organization.Organization" table="TR_ORGANIZATION">
		<id name="id" type="int">
			<column name="IDTR_ORGANIZATION" precision="5" scale="0" />
			<generator class="increment" />
		</id>
		<property name="name" type="string">
			<column name="TR_ORGA_COMPANY_NAME" length="45" not-null="true" />
		</property>
		<property name="streetNumber" type="string">
			<column name="TR_ORGA_STREET_NUMBER" length="45" not-null="true" />
		</property>
		<property name="streetType" type="String">
			<column name="TR_ORGA_STREET_TYPE" length="45" not-null="true" />
		</property>
		<property name="streetName" type="String">
			<column name="TR_ORGA_STREET_NAME" length="45" not-null="true" />
		</property>
		<property name="zipCode" type="String">
			<column name="TR_ORGA_ZIP_CODE" length="45" not-null="true" />
		</property>
		<property name="city" type="String">
			<column name="TR_ORGA_CITY" length="45" not-null="true" />
		</property>
		<property name="department" type="String">
			<column name="TR_ORGA_DEPARTMENT" length="45" not-null="true" />
		</property>
		<property name="phone" type="String">
			<column name="TR_ORGA_TEL_NUMBER" length="45" not-null="true" />
		</property>
		<property name="siret" type="String">
			<column name="TR_ORGA_SIRET" length="45" not-null="true" />
		</property>
		<property name="fax" type="String">
			<column name="TR_ORGA_FAX" length="45" not-null="true" />
		</property>
		<property name="email" type="String">
			<column name="TR_ORGA_EMAIL" length="255" not-null="true" />
		</property>
		<property name="latitude" type="Float">
			<column name="TR_ORGA_LATITUDE" length="45" not-null="true" />
		</property>
		<property name="longitude" type="Float">
			<column name="TR_ORGA_LONGITUDE" length="45" not-null="true" />
		</property>
		<property name="updateDate" type="date">
			<column name="UPDATEDATE" length="45" not-null="false" />
		</property>
		<property name="createDate" type="date">
			<column name="CREATEDATE" length="45" not-null="true" />
		</property>
		<property name="updateUser" type="string">
			<column name="UPDATEUSER" length="45" not-null="true" />
		</property>	
		
		<set name="servicesSet" table="tr_orga_serv_association"
			inverse="true" lazy="true" fetch="select" cascade="all">
			<key>
				<column name="IDTR_ORGANIZATION" not-null="true"/>
			</key>
			<many-to-many entity-name="fr.esiag.isies.pds.model.referential.ServiceType">
				<column name="TR_SERVICE_TYPE_CODE" not-null="true"/>
			</many-to-many>
		</set>
		<many-to-one name="orgaType" column="IDTR_ORGANIZATION_TYPE"
			class="OrgaType" not-null="true"/>
		<joined-subclass name="Hospital" table="HEALTH_ORGANIZATION">
			<key column="IDTR_ORGANIZATION"/>
			<property name="finess" column="TR_HOSPITAL_ORGA_FINESS"/>
		</joined-subclass>
	</class>
</hibernate-mapping>